    "faucet": "^0.0.1",
    "rollup": "^0.45.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "ramda": "^0.24.1",
    "recompose": "^0.24.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "learn2code"
  ],
  "author": "Tom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilson63/codepix.git"
